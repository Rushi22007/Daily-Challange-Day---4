#include<stdio.h>

static int count[100001];
static int sum[100001];
static int built = 0;

void solve() {
    if (built) return;
    built = 1;

    for (int i = 1; i <= 100000; i++) {
        count[i] = 0;
        sum[i] = 0;
    }

    for (int i = 1; i <= 100000; i++) {
        for (int j = i; j <= 100000; j += i) {
            count[j]++;
            sum[j] += i;
        }
    }
}

int sumFourDivisors(int* nums, int numsSize) {
    solve();
    int ans = 0;

    for (int i = 0; i < numsSize; i++) {
        int x = nums[i];
        if (count[x] == 4) {
            ans += sum[x];
        }
    }
    return ans;
}
